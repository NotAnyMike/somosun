<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	
	</ui:style>
	<ui:style field="second" src="styles/styles.css"/>
	<ui:style field="third" src="styles/bootstrap.css"/>
	<ui:style field="forth" src="styles/bootstrap-theme.min.css"/>
	<ui:style field="fifth" src="styles/malla.css"/>
	<g:HTMLPanel>
	    
	    
<div class="transparent_background">

</div>
<div class="warning_box">
<table border="0" cellpadding="0" cellspacing="0" class="warning">
  <tr>
    <td height="30" colspan="5"></td>
  </tr>
  <tr>
    <td width="40"></td>
    <td align="center" valign="middle">
        <div class="subjectBox">
            <div class=" topSubjectBox"><g:Label ui:field="subjectCode">34567890</g:Label></div>
            <div class=" mainSubjectBox wrapword "><g:Label ui:field="subjectName"></g:Label></div>
            <div class="bottomSubjectBox">
                <div class="gradeBottomSubjectBox"><g:Label ui:field="subjectGrade">-</g:Label></div>
                <div class=" creditsBottomSubjectBox "><g:Label ui:field="subjectCredits">c: </g:Label></div>
                <div class="obligBottomSubjectBox"><g:Label ui:field="subjectOblig">Oblig: Si</g:Label></div>
                <div class="typeBottomSubjectBox"><g:Label ui:field="subjectType"></g:Label></div>
            </div>
        </div>
    </td>
    <td width="40" align="center" valign="middle"></td>
    <td align="center" valign="middle"><table border="0" cellspacing="0" cellpadding="0">
      <tr>
        <td align="left" valign="middle"><g:Button ui:field="cancelButton" addStyleNames="{third.btn} {forth.btn-info}">Cancelar</g:Button></td>
      </tr>
      <tr>
        <td height="30" align="left" valign="middle"></td>
      </tr>
      <tr>
        <td align="left" valign="middle"><g:Button ui:field="deleteButton" addStyleNames="{third.btn} {forth.btn-warning}">Eliminar</g:Button>
</td>
      </tr>
    </table></td>
    <td width="40"></td>
  </tr>
  <tr>
    <td height="30" colspan="5"></td>
  </tr>
</table>
</div>
	    
	    
	    
	</g:HTMLPanel>
</ui:UiBinder> 