<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:b.html="urn:import:org.gwtbootstrap3.client.ui.html">
	<ui:style>
	
	</ui:style>
	<g:HTMLPanel>
	    <div class="row">
				<div class="col-xs-12 col-sm-4 col-lg-3">
					<div class="panel panel-default">
						<div class="panel-heading">Career</div>
						<div class="panel-body">
							<b:VerticalButtonGroup>
								<b:Button type="WARNING" ui:field="resetAllHasAnalysisButton" g:id="goas">Reset all hasAnalysis</b:Button>
								<b:InputGroup>
									<b:ListBox ui:field="careerAnalysisListBox"></b:ListBox>
									<b:InputGroupButton>									
										<b:Button type="WARNING" ui:field="resetCertainHasAnalysisButton">hasAnalysis=0</b:Button>
									</b:InputGroupButton>
								</b:InputGroup>
								<b:Button type="DANGER" ui:field="resetAllHasDefaultButton" enabled="true">Reset all hasDefault</b:Button>
								<b:InputGroup>
									<b:ListBox ui:field="careerDefaultListBox"></b:ListBox>
									<b:InputGroupButton>									
										<b:Button type="WARNING" ui:field="resetCertainHasDefaultButton" enabled="true">hasDefault=0</b:Button>
									</b:InputGroupButton>
								</b:InputGroup>
								<b:Button type="DEFAULT" ui:field="resetUpdateCareersButton">Reset (update) careers</b:Button>
								<b:InputGroup>
									<b:ListBox ui:field="careerAnalyseListBox"></b:ListBox>
									<b:InputGroupButton>									
										<b:Button type="WARNING" ui:field="analyseCertainCareerButton" enabled="true">Analyse</b:Button>
									</b:InputGroupButton>
								</b:InputGroup>
								<b:Button type="WARNING" ui:field="analyseAllCareersButton">- Analyse ALL careers -</b:Button>
								<b:Button type="INFO" ui:field="analyseCareersNotAnalyzedButton">- Analyse careers not analyzed -</b:Button>
							</b:VerticalButtonGroup>
						</div>
					</div>
				</div>
				<div class="col-xs-12 col-sm-4 col-lg-3">
					<div class="panel panel-default">
						<div class="panel-heading">ComplementaryValue</div>
						<div class="panel-body">
							<b:VerticalButtonGroup>
								<b:Button type="DANGER" ui:field="deleteAllComplementaryValuesButton" enabled="true">- Delete ALL -</b:Button>
								<b:InputGroup>
									<b:InputGroupButton>									
										<b:Button type="WARNING" ui:field="deleteCertainComplementaryValuesButton" enabled="true">delete for:</b:Button>
									</b:InputGroupButton>
									<b:ListBox ui:field="complementaryValuesListBox"></b:ListBox>
								</b:InputGroup>
							</b:VerticalButtonGroup>
						</div>
					</div>
				</div>
				<div class="col-xs-12 col-sm-4 col-lg-3">
					<div class="panel panel-default">
						<div class="panel-heading">Plan</div>
						<div class="panel-body">
							<b:VerticalButtonGroup>
								<b:Button type="DANGER" ui:field="deleteAllDefaultPlansButton">- Delete ALL default plans -</b:Button>
								<b:InputGroup>
									<b:InputGroupButton>									
										<b:Button type="WARNING" ui:field="deleteCertainDefaultPlanButton">delete default plan for</b:Button>
									</b:InputGroupButton>
									<b:ListBox ui:field="planListBox"></b:ListBox>
								</b:InputGroup>
								<b:Button type="DANGER" ui:field="deleteAllPlansButton">- Delete ALL plans -</b:Button>
								<b:InputGroup>
								    <b:TextBox></b:TextBox>
									<b:InputGroupButton>									
										<b:Button type="DEFAULT" enabled="false">Open plan</b:Button>
									</b:InputGroupButton>
								</b:InputGroup>
							</b:VerticalButtonGroup>
						</div>
					</div>
				</div>
				
				<div class="col-xs-12 col-sm-4 col-lg-3">
					<div class="panel panel-default">
						<div class="panel-heading">Semester</div>
						<div class="panel-body">
							<b:VerticalButtonGroup>
								<b:Button type="DANGER" ui:field="deleteAllSemestersButton" enabled="true">- Delete ALL semesters -</b:Button>
								<b:Button type="DEFAULT" enabled="false">Change current semester</b:Button>
							</b:VerticalButtonGroup>
						</div>
					</div>
				</div>
				
				<div class="col-xs-12 col-sm-4 col-lg-3">
					<div class="panel panel-default">
						<div class="panel-heading">Subject</div>
						<div class="panel-body">
							<b:VerticalButtonGroup>
								<b:Button type="DANGER" ui:field="deleteAllSubjectsButton" enabled="true"> - Delete ALL subjects -</b:Button>
								<b:InputGroup>
									<b:TextBox></b:TextBox>
									<b:InputGroupButton>
										<b:Button type="DEFAULT" enabled="false">Edit</b:Button>
									</b:InputGroupButton>
								</b:InputGroup>
							</b:VerticalButtonGroup>
						</div>
					</div>
				</div>
				<div class="col-xs-12 col-sm-4 col-lg-3">
					<div class="panel panel-default">
						<div class="panel-heading">SubjectGroup</div>
						<div class="panel-body">
							<b:VerticalButtonGroup>
								<b:Button type="DANGER" ui:field="deleteAllSubjectGroupsButton" enabled="true"> - Delete ALL subjectGroups -</b:Button>
								<b:InputGroup>
									<b:InputGroupButton>									
										<b:Button type="WARNING" enabled="false">Delete all for</b:Button>
									</b:InputGroupButton>
									<b:ListBox ui:field="subjectGroupListBox"></b:ListBox>
								</b:InputGroup>
							</b:VerticalButtonGroup>
						</div>
					</div>
				</div>
				<div class="col-xs-12 col-sm-4 col-lg-3">
					<div class="panel panel-default">
						<div class="panel-heading">SubjectValue</div>
						<div class="panel-body">
							<b:VerticalButtonGroup>
								<b:Button type="DANGER" ui:field="deleteAllSubjectValueButton" enabled="true"> - Delete ALL subjectValues -</b:Button>
							</b:VerticalButtonGroup>
						</div>
					</div>
				</div>
				<div class="col-xs-12 col-sm-4 col-lg-3">
					<div class="panel panel-default">
						<div class="panel-heading">Errors</div>
						<div class="panel-body">
							<b:VerticalButtonGroup>
								<b:Button type="DEFAULT" enabled="false">Clean all</b:Button>
							</b:VerticalButtonGroup>
						</div>
					</div>
				</div>
				<div class="col-xs-12 col-sm-4 col-lg-3">
					<div class="panel panel-default">
						<div class="panel-heading">User</div>
						<div class="panel-body">
							<b:VerticalButtonGroup>
								<b:InputGroup>
								    <b:TextBox ui:field="userToUnBlockTextBox"></b:TextBox>
									<b:InputGroupButton>									
										<b:Button type="WARNING" enabled="true" ui:field="unBlockUserButton">(Un)Block User</b:Button>
									</b:InputGroupButton>
								</b:InputGroup>
								<b:InputGroup>
								    <b:TextBox ui:field="userCodeTextBox"></b:TextBox>
									<b:InputGroupButton>									
										<b:Button type="DEFAULT" enabled="true" ui:field="makeUserAdminButton">(Un)Make user admin</b:Button>
									</b:InputGroupButton>
								</b:InputGroup>
							</b:VerticalButtonGroup>
						</div>
					</div>
				</div>
			</div>
	</g:HTMLPanel>
</ui:UiBinder> 