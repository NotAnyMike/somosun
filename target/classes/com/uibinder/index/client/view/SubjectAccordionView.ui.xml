 			    <b:Panel ui:field="accordionContainer">
				    <b:PanelHeader dataParent="#accordion2" dataToggle="COLLAPSE" dataTarget="#collapseOne" ui:field="header">
				      <b:Heading size="H4">
					      <g:HorizontalPanel>
					      	<b:Button b:id="btnAddSpecificSubject" ui:field="addSpecificSubject" type="SUCCESS" icon="PLUS" size="EXTRA_SMALL"/>
					      	<g:Label ui:field="codeFieldSearchSubject"></g:Label>
					      	<g:Label ui:field="nameFieldSearchSubject"></g:Label>
					      	<g:HorizontalPanel ui:field="infoHolderSearchSubject">
						      	<g:Label ui:field="typeFieldSearchSubject">L</g:Label>
						      	<g:Label ui:field="creditsFieldSearchSubject"></g:Label>
					      	</g:HorizontalPanel>
					      </g:HorizontalPanel>
				      </b:Heading>
				    </b:PanelHeader>
				    <b:PanelCollapse ui:field="accordionBody" b:id="collapseOne">
				      <b:PanelBody>
				        <b.html:Paragraph>
                               
                               <table width="100%" border="0" cellpadding="0" cellspacing="0">
                               	<tr>
                               		<td valing="top">
                               			<div class="form-group" style="width:460px">
							                <div class="input-group">
							                  <div class="input-group-addon">Mostrar requisitos para</div>
							                  <g:ListBox ui:field="listBoxCareers" addStyleNames="{second.textboxCreate} {third.lead} "></g:ListBox>
							                  </div>
							              </div>
                               		</td>
                               		<td></td>
                               		<td width="300" valing="top" aling="center" style="vertical-align: top;">
                               			<div class="panel panel-default" style="">
							              <div class="panel-body text-center" style="padding:6px">
							                <g:Label ui:field="labelSubjectGroup">Agrupacion: Contenido cuantitativo</g:Label>
							              </div>
							          </div>
                               		</td>
                               	</tr>
                               	<tr>
                               		<td></td>
                               		<td width="10"></td>
                               		<td></td>
                               	</tr>
                               	<tr height="10px">
                               		<td colspan="3"></td>
                               	</tr>
                               	<tr>
                               		<td colspan="3" width="100%">
                               			<table border="0" cellspacing="0" cellpadding="0" class="table table-bordered" style="padding:0px">
								          <tr>
								            <td align="center" valign="middle"><small>pre-requisitos</small></td>
								            <td align="center" valign="middle"><small>co-requisitos</small></td>
								          </tr>
								          <tr>
								            <td style="padding:0px; margin-bottom:0px">
									            <g:VerticalPanel ui:field="preRequisitesPanel">
									                <b:Icon type="REFRESH" spin="true"/>
									            </g:VerticalPanel>
								            </td>
								            <td style="padding:0px">
									            <g:VerticalPanel ui:field="coRequisitesPanel">
									                <b:Icon type="REFRESH" spin="true"/>
									            </g:VerticalPanel>
								            </td>
								          </tr>
								        </table>
								        
								        <p></p>
								        
								        <div class="panel panel-default text-left" style="">
							                <div class="panel-body" style="padding:6px">
							                  <table>
							                    <tr>
							                      <td style="min-width: 225px" aling="left" valign="top">
							                        <div data-toggle="tooltip" data-placement="top" title="Se requiere de la clase seleccionada para ver las siguientes clases">Modelacion estatica es pre-requisito de:</div> 
							                      </td>
							                      <td width="10px"></td>
							                      <td aling="left" valing="top" style="margin:1px">
							                      	<g:FlowPanel ui:field="antiPreRequisite">
							                      	    <b:Icon type="REFRESH" spin="true"/>
							                      	</g:FlowPanel>
							                      </td>
							                    </tr>
							                    <tr>
							                    	<tr colspan="3" height="10"></tr>
							                    </tr>
							                    <tr>
							                    	<td style="min-width: 230px" aling="left" valign="top">
							                        	<div data-toggle="tooltip" data-placement="top" title="Si quieres ver las siguientes clases, debes estar cursando o haber cursado la clase seleccionada">Modelación estatica es co-requisito de:</div> 
							                      	</td>
							                    	<td width="10px"></td>
								                    <td aling="left" valing="top" style="margin:1px">
								                    	<g:FlowPanel ui:field="antiCoRequisite">
								                    	    <b:Icon type="REFRESH" spin="true"/>
								                    	</g:FlowPanel>
								                    </td>
							                    </tr>
							                  </table>
							                </div>
							              </div>
								        
                               		</td>	
                               	</tr>
                               </table>
					        
					        <br></br>
					        
					        <g:VerticalPanel ui:field="groupTableContainer">
					        	<g:Grid ui:field="groupTableTitles" styleName="table">
						        	<g:row>
						        		<g:cell styleName="text-center"># gr</g:cell>
						        		<g:cell styleName="text-left"><strong>Profesor</strong></g:cell>
						        		<g:cell styleName="text-center"><div title="Cuan buen profesor es" data-toggle="tooltip" data-placement="left"><small>Nota profesor</small></div></g:cell>
						        		<g:cell styleName="text-center"><div title="Cual fue la nota definitiva promedio de este grupo en el SIA"  data-toggle="tooltip" data-placement="top"><small>Nota promedio</small></div></g:cell>
						        		<g:customCell styleName="text-center">
						        			<g:Grid ui:field="spacesTable">
						        				<g:row>
						        					<g:cell styleName="text-center"><small>Cupos</small></g:cell>
						        				</g:row>
						        				<g:row>
						        					<g:customCell  styleName="text-center">
							        					<g:Grid ui:field="titleSpacesTable">
								        					<g:row>
								        						<g:cell styleName="text-center"><small>disp</small></g:cell>
								        						<g:cell styleName="text-center"><small>total</small></g:cell>
								        					</g:row>
							        					</g:Grid>
						        					</g:customCell>
						        				</g:row>
						        			</g:Grid>
						        		</g:customCell>
						        		<g:cell styleName="text-center"><strong>L</strong></g:cell>
						        		<g:cell styleName="text-center"><strong>M</strong></g:cell>
						        		<g:cell styleName="text-center"><strong>C</strong></g:cell>
						        		<g:cell styleName="text-center"><strong>J</strong></g:cell>
						        		<g:cell styleName="text-center"><strong>V</strong></g:cell>
						        		<g:cell styleName="text-center"><strong>S</strong></g:cell>
						        		<g:cell styleName="text-center"><strong>D</strong></g:cell>
						        	</g:row>
						        </g:Grid>
						        <b:Icon type="REFRESH" spin="true"/>
				        </g:VerticalPanel>
					        
						<br></br>
							
						<div class="alert alert-warning" role="alert">Quieres ayudar a completar estas funcionalidades, entonces has click <a>aquí</a></div>
                                  
                           </b.html:Paragraph>
				      </b:PanelBody>
				    </b:PanelCollapse>
				</b:Panel>
 			    
		<!-- 	</body>
		</html> -->
	</g:HTMLPanel>
</ui:UiBinder> 