<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.7.0//EN" "http://google-web-toolkit.googlecode.com/svn/tags/2.7.0/distro-source/core/src/gwt-module.dtd">
<module rename-to="admin">
	<inherits name="com.google.gwt.user.User" />

	<!--  DND support -->
	<inherits name="com.allen_sauer.gwt.dnd.gwt-dnd"/>
  
	<!-- Bootstrap GWT support -->
	<inherits name="org.gwtbootstrap3.GwtBootstrap3"/>
	<inherits name="org.gwtbootstrap3.extras.respond.Respond"/>
  
	<!-- Guava -->
	<inherits name="com.google.common.collect.Collect" />	
	
	<entry-point class="com.somosun.plan.client.admin.Admin"></entry-point>
	
	<source path="client" />
	<source path="shared" />
</module>