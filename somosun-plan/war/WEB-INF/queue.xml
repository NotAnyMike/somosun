<queue-entries>
<!--Change the refresh rate of the default queue to 1/s-->
  <queue>
    <name>analyseCareer</name>
    <rate>6/h</rate>
    <bucket-size>1</bucket-size>
    <max-concurrent-requests>1</max-concurrent-requests>
    <retry-parameters>        
    	<task-retry-limit>5</task-retry-limit>
    </retry-parameters>
  </queue>
  <queue>
    <name>analyseAllCareers</name>
    <rate>1/s</rate>
    <max-concurrent-requests>1</max-concurrent-requests>
    <bucket-size>1</bucket-size>
    <retry-parameters>        
    	<task-retry-limit>5</task-retry-limit>
    </retry-parameters>
  </queue>
  <queue>
    <name>adminQueue</name>
    <rate>1/m</rate>
    <bucket-size>1</bucket-size>
    <max-concurrent-requests>5</max-concurrent-requests>
    <retry-parameters>        
    	<task-retry-limit>5</task-retry-limit>
    </retry-parameters>
  </queue>
  <queue>
  	<name>updateSubjectGrade</name>
  	<mode>pull</mode>
  	<retry-parameters>
  		<task-retry-limit>5</task-retry-limit>
  	</retry-parameters>
  </queue>
</queue-entries>