<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:b.html="urn:import:org.gwtbootstrap3.client.ui.html">
	<ui:style>
	
	</ui:style>
	<g:HTMLPanel>
	    <div class="row">
				<div class="col-xs-12 col-sm-4 col-lg-3">
					<div class="panel panel-default">
						<div class="panel-heading">Career</div>
						<div class="panel-body">
							<b:VerticalButtonGroup>
								<b:Button type="WARNING">Reset all hasAnalysis</b:Button>
								<b:InputGroup>
									<b:ListBox ui:field="careerAnalysisListBox"></b:ListBox>
									<b:InputGroupButton>									
										<b:InputButton type="WARNING">hasAnalysis=0</b:InputButton>
									</b:InputGroupButton>
								</b:InputGroup>
								<b:Button type="DANGER">Reset all hasDefault</b:Button>
								<b:InputGroup>
									<b:ListBox ui:field="careerDefaultListBox"></b:ListBox>
									<b:InputGroupButton>									
										<b:InputButton type="WARNING">hasDefault=0</b:InputButton>
									</b:InputGroupButton>
								</b:InputGroup>
								<b:Button type="DEFAULT">Reset (update) careers</b:Button>
							</b:VerticalButtonGroup>
						</div>
					</div>
				</div>
				<div class="col-xs-12 col-sm-4 col-lg-3">
					<div class="panel panel-default">
						<div class="panel-heading">ComplementaryValues</div>
						<div class="panel-body">
							<b:VerticalButtonGroup>
								<b:Button type="DANGER">Delete all</b:Button>
								<b:InputGroup>
									<b:InputGroupButton>									
										<b:InputButton type="WARNING">delte for:</b:InputButton>
									</b:InputGroupButton>
									<b:ListBox ui:field="complementaryValuesListBox"></b:ListBox>
								</b:InputGroup>
							</b:VerticalButtonGroup>
						</div>
					</div>
				</div>
				<div class="col-xs-12 col-sm-4 col-lg-3">
					<div class="panel panel-default">
						<div class="panel-heading">Plan</div>
						<div class="panel-body">
							<b:VerticalButtonGroup>
								<b:Button type="DANGER" ui:field="deleteAllDefaultPlansButton">Delete all default plans</b:Button>
								<b:InputGroup>
									<b:InputGroupButton>									
										<b:InputButton type="WARNING" ui:field="deleteCertainDefaultPlanButton">delete default plan for</b:InputButton>
									</b:InputGroupButton>
									<b:ListBox ui:field="planListBox"></b:ListBox>
								</b:InputGroup>
							</b:VerticalButtonGroup>
						</div>
					</div>
				</div>
				
				<div class="col-xs-12 col-sm-4 col-lg-3">
					<div class="panel panel-default">
						<div class="panel-heading">Semester</div>
						<div class="panel-body">
							<b:VerticalButtonGroup>
								<b:Button type="DEFAULT">Change current semester</b:Button>
							</b:VerticalButtonGroup>
						</div>
					</div>
				</div>
				
				<div class="col-xs-12 col-sm-4 col-lg-3">
					<div class="panel panel-default">
						<div class="panel-heading">Subject</div>
						<div class="panel-body">
							<b:VerticalButtonGroup>
								<b:InputGroup>
									<b:TextBox></b:TextBox>
									<b:InputGroupButton>
										<b:InputButton>Edit</b:InputButton>
									</b:InputGroupButton>
								</b:InputGroup>
								<b:InputGroup>
									<b:InputGroupButton>									
										<b:InputButton type="WARNING">Delete all for</b:InputButton>
									</b:InputGroupButton>
									<b:ListBox ui:field="subjectListBox"></b:ListBox>
								</b:InputGroup>
							</b:VerticalButtonGroup>
						</div>
					</div>
				</div>
				<div class="col-xs-12 col-sm-4 col-lg-3">
					<div class="panel panel-default">
						<div class="panel-heading">SubjectGroup</div>
						<div class="panel-body">
							<b:VerticalButtonGroup>
								<b:Button type="DANGER">Delete ALL</b:Button>
								<b:InputGroup>
									<b:InputGroupButton>									
										<b:InputButton type="WARNING">Delete all for</b:InputButton>
									</b:InputGroupButton>
									<b:ListBox ui:field="subjectGroupListBox"></b:ListBox>
								</b:InputGroup>
							</b:VerticalButtonGroup>
						</div>
					</div>
				</div>
				<div class="col-xs-12 col-sm-4 col-lg-3">
					<div class="panel panel-default">
						<div class="panel-heading">Errors</div>
						<div class="panel-body">
							<b:VerticalButtonGroup>
								<b:Button type="DEFAULT">Clean all</b:Button>
							</b:VerticalButtonGroup>
						</div>
					</div>
				</div>
			</div>
	</g:HTMLPanel>
</ui:UiBinder> 