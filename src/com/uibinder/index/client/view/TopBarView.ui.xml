<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:b.html="urn:import:org.gwtbootstrap3.client.ui.html">
	<ui:style field="first">
		
	</ui:style>
	<ui:style field="second" src="styles/styles.css"/>
	<ui:style field="third" src="styles/bootstrap.css"/>
	<ui:style field="forth" src="styles/malla.css"/>
	<g:HTMLPanel>
	    <div class="navbar navbar-inverse navbar-fixed-top" role="navigation" style="font-size:14px;">
	    	<div class="container">
				<div class="navbar-header">
					<g:Button styleName="{third.navbar-toggle}" addStyleNames="{third.inline}">
						<div class="sr-only inline">
							Toggle navigation
						</div>
						<div class="sr-only inline"></div>
						<div class="sr-only inline"></div>
						<div class="sr-only inline"></div>
					</g:Button>
					<a class="navbar-brand" href="#index"><span class="glyphicon glyphicon-home"></span></a>
					<p class="navbar-text" style="cursor:default">#SomosUN</p>
	    		</div>
	    		<div class="collapse navbar-collapse">
				<ul class="nav navbar-nav navbar-right">
					<p class="navbar-text" style="cursor:default"><g:Label ui:field="userNameLabel"></g:Label></p>
					<li id="log-hyperlink"><b:Anchor ui:field="logAnchor"></b:Anchor></li>
					<li id="panel"><b:Anchor ui:field="adminPanelAnchor"></b:Anchor></li>
				</ul>
				<g:Label ui:field="pageNameLabel" addStyleNames="{third.lead} {third.main-text} {third.navbar-text} {forth.mainLabelNamePageTopBar} "></g:Label>
			</div>
	    	</div>
	    </div>
	</g:HTMLPanel>
</ui:UiBinder> 