<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
              http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5"
         xmlns="http://java.sun.com/xml/ns/javaee">

  <!-- Servlets -->
  <servlet>
      <servlet-name>SUNServiceImpl</servlet-name>
      <servlet-class>com.uibinder.index.server.serviceImpl.SUNServiceImpl</servlet-class>
  </servlet>
  
  <servlet-mapping>
      <servlet-name>SUNServiceImpl</servlet-name>
      <url-pattern>/uibinderindex/sunService</url-pattern>
  </servlet-mapping>
  
  <servlet>
      <servlet-name>LoginService</servlet-name>
      <servlet-class>com.uibinder.index.server.serviceImpl.LoginServiceImpl</servlet-class>
  </servlet>
  
  <servlet-mapping>
      <servlet-name>LoginService</servlet-name>
      <url-pattern>/uibinderindex/loginService</url-pattern>
  </servlet-mapping>
  
  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>UiBinderIndex.html</welcome-file>
  </welcome-file-list>

</web-app>
